<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title || 'Document' %></title>
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/admin/admin-navbar.css">
    <link rel="stylesheet" href="/css/admin/custom.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"/>
</head>
<body>
    <% if(!success){ %>
        <div class="alert alert-danger">
            <%= message %>
        </div>
        <% } else { %>
            <div id="wrapper">
                <%- include('../components/navbar.ejs', { primary: primary }) %>
                <div id="content-wrapper" class="d-flex flex-column">
                    <div class="container-fluid">
                        <div class="d-sm-flex align-items-center justify-content-between mb-4 mt-3">
                            <h1 class="h3 mb-0"><%= title %></h1>
                        </div>
                        <%- include('../forms/add-consultation-form.ejs') %>
                        <div id="getSchedule">
                        </div>
                    </div>
                </div>
            </div>
        <% } %>
</body>
<script src="/js/admin/animation.js"></script>
<script src="/js/admin/consultations/get-timeslots.js"></script>
<script src="/js/admin/consultations/walkin-consultation.js"></script>
</html>

